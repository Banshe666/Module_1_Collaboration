   "source": [
    "from sqlalchemy import create_engine, text\n",
    "\n",
    "my_db = create_engine('sqlite:///books.db')\n",
    "\n",
    "with my_db.connect() as connection:\n",
    "    result = connection.execute(text(\"SELECT title FROM books ORDER BY title\"))\n",
    "    \n",
    "    for row in result:\n",
    "        print(row[0])\n"
   ]
  }
